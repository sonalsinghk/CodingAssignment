for(int i=2;i<=n;i++){
   for(int j=2;j<n;j++){
      if(n%i==0){
         break;
      }
      if(j=n){
        cout<<n;
      }
   }
}
Code1:
#include<iostream>
using namespace std;
bool prime(int n){
	if(n==1){
		return false;
	}
	for(int i=2;i<n;i++){
		if(n%i==0){
			return false;
		}
	}
	return true;
}
int main() {
	int n;
	cin>>n;
	for(int i=2;i<=n;i++){
        if(prime(i)){
			cout<<i<<endl;
		}
	}
	return 0;
} 















